    <span>\n                {{item.title}}\n            </span>\n        </div>\n       </div>\n    </div>\n</template>\n<script lang=\"ts\">\nimport { BBadge } from \"bootstrap-vue\";\n// eslint-disable-next-line no-unused-vars\nimport { Component, Vue, Prop, Watch } from \"vue-property-decorator\";\n@Component({\n  components: {\n    BBadge,\n  },\n  // mounted(){\n  //   this.cou\n  // }\n})\nexport default class OverviewItem extends Vue {\n  delay = 50;\n  time = 1000;\n  value = 0;\n\n  @Prop({ default: null })\n  private item!: any;\n  counterUp() {\n    this.value = 0;\n    let unit = Math.floor((this.item.value / this.time) * this.delay);\n    if (unit < 1) unit = 1;\n    setInterval(() => {\n      this.value += unit;\n      if (this.value >= this.item.value) {\n        this.value = this.item.value;\n        clearInterval();\n      }\n    }, this.delay);\n  }\n  @Watch(\"item\")\n  onItemChange(newItem: object) {\n    if (newItem) this.counterUp();\n  }\n\n  mounted() {\n    this.counterUp();\n  }\n}\n</script>\n<style lang=\"scss\">\n.total-overview-item {\n  padding: 20px 10px;\n  background: #fafbfd;\n  margin: 10px 0px;\n  border-radius: 0.75rem;\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  .badge {\n    width: 50px;\n    height: 50px;\n    border-radius: 50%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .result {\n    font-weight: 600;\n    font-size: 21px;\n  }\n  .description {\n    font-size: 12px;\n  }\n}\n</style>"]}]}