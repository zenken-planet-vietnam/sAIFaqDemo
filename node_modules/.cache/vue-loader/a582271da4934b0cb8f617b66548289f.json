if=\"type!=='result'\">\n            <span>{{data.label}}</span>\n          </div>\n          <high-light :text=\"data.label\" :targets=\"getTargets\" v-else/>\n      </div>\n    </div>\n</template>\n<script lang=\"ts\">\nimport HighLight from \"./HighLight.vue\";\nimport PageMixin from \"@/@core/mixins/searchDataMixin\";\nimport { PageModule } from \"@/store/modules/page\";\nimport { Component, Prop } from \"vue-property-decorator\";\nimport { mixins } from \"vue-class-component\";\n@Component({\n  components: {\n    HighLight,\n  },\n})\nexport default class QuestionItem extends mixins(PageMixin) {\n  @Prop({ default: null })\n  private data!: object;\n  @Prop({ default: \"frequent\" })\n  private type!: String;\n\n  get getTargets() {\n    return this?.searchWords;\n  }\n\n  getAnswerFromQuestion(data: any) {\n    // call analytics api\n    let sa = (window as any).sa;\n    if (sa) {\n      let sendData = {\n        event_name: \"question_click\",\n        value: {\n          clicked_id: data.id,\n          clicked_text: data.label,\n        },\n      };\n      sa.send(sendData);\n    }\n    PageModule.updateProcess(false);\n    this.$router.push({ name: \"result-page\", query: { id: data.id } });\n  }\n}\n</script>\n<style lang=\"scss\">\n.question-item {\n  padding: 10px 20px;\n  border-bottom: 1px solid lightgray;\n  cursor: pointer;\n  transition: all 2s linear;\n  .question-content {\n    display: flex;\n    align-items: center;\n    .icon {\n      color: #28c76f;\n      margin-right: 10px;\n    }\n  }\n  /* Enter and leave animations can use different */\n  /* durations and timing functions.              */\n  .slide-fade-enter-active {\n    transition: all 0.5s ease;\n  }\n  .slide-fade-leave-active {\n    transition: all 0.1s cubic-bezier(1, 0.5, 0.8, 1);\n  }\n  .slide-fade-enter, .slide-fade-leave-to\n/* .slide-fade-leave-active below version 2.1.8 */ {\n    transform: translateX(10px);\n    opacity: 0;\n  }\n}\n</style>"]}]}